<Window x:Class="DoctorClient.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DoctorClient"
        mc:Ignorable="d"
        Title="LoginWindow" Height="450" Width="800">
    <Grid>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Center" Margin="0,151,0,0" VerticalAlignment="Top" Width="120"/>
        <Button Content="Login" HorizontalAlignment="Center" Margin="0,197,0,0" VerticalAlignment="Top" Click="LoginButton_Click"/>

    </Grid>
</Window>
